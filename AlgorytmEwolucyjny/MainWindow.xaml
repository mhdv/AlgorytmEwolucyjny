<Window x:Class="AlgorytmEwolucyjny.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlgorytmEwolucyjny"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<TextBox x:Name="txtEquation" HorizontalAlignment="Left" Height="22" Margin="10,10,0,0" TextWrapping="Wrap" Text="x1^4+x2^4-0.62*x1^2-0.62*x2^2" VerticalAlignment="Top" Width="772"/>
		<Button x:Name="btnEquation" Content="Zatwierdź" HorizontalAlignment="Left" Margin="707,65,0,0" VerticalAlignment="Top" Width="75" Click="btnEquation_Click"/>
		<TextBox x:Name="tmpSolution" HorizontalAlignment="Left" Margin="10,37,0,0" TextWrapping="Wrap" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Text="Rozwiązanie: " VerticalAlignment="Top" Height="372" Width="647"/>
		<TextBox x:Name="txtPopulationSize" HorizontalAlignment="Left" Height="23" Margin="662,37,0,0" TextWrapping="Wrap" Text="10" MaxLength="3" VerticalScrollBarVisibility="Auto" VerticalAlignment="Top" Width="120" PreviewTextInput="PreviewTextInput"/>

	</Grid>
</Window>
